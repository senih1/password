@model User
@if (TempData["AuthError"] != null)
{
    <div class="alert @TempData["AuthErrorCss"] mb-5" role="alert">
        @TempData["AuthError"]
    </div>
}
    <h2>Token</h2>
    <form action="/token" method="post">
        <p><input type="text" placeholder="Token" name="token" required /></p>
        <button type="submit">İstek Gönder</button>
    </form>
